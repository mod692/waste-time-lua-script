-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ğ¸
local function UpdateStatus(text, color)
    if not ScriptEnabled then return end
    StatusLabel.Text = "âœ… Status: " .. text
    StatusLabel.TextColor3 = color or Color3.fromRGB(100, 255, 100)
end

local function UpdateCodesStats()
    if not ScriptEnabled then return end
    CodesLabel.Text = string.format("ğŸ Codes Redeemed: %d/%d\nğŸ“‹ Total Codes: %d", #RedeemedCodes, #ActiveCodes, #ActiveCodes)
end

local function StopAllScripts()
    ScriptEnabled = false
    Settings.AutoRedeemCodes = false
    
    UpdateStatus("Script Disabled", Color3.fromRGB(255, 100, 100))
    wait(0.5)
    
    if ScreenGui then
        ScreenGui:Destroy()
    end
    
    print("Waste-Time Auto Codes fully disabled and removed!")
end

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ°ĞºÑ‚Ğ¸Ğ²Ğ°Ñ†Ğ¸Ğ¸ ĞºĞ¾Ğ´Ğ°
local function RedeemCode(code)
    if not ScriptEnabled then return false end
    if table.find(RedeemedCodes, code) then
        return false
    end
    
    local success = false
    
    pcall(function()
        print("ğŸ”„ Attempting to redeem code: " .. code)
        
        -- ĞœĞµÑ‚Ğ¾Ğ´ 1: ĞŸĞ¾Ğ¸ÑĞº Ğ² ReplicatedStorage
        for _, v in pairs(ReplicatedStorage:GetDescendants()) do
            if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
                local name = v.Name:lower()
                if name:find("code") or name:find("redeem") or name:find("promo") then
                    print("ğŸ“¡ Found remote: " .. v.Name)
                    
                    if v:IsA("RemoteEvent") then
                        v:FireServer(code)
                        v:FireServer("Redeem", code)
                        v:FireServer({Code = code})
                    else
                        v:InvokeServer(code)
                    end
                    success = true
                end
            end
        end
        
        -- ĞœĞµÑ‚Ğ¾Ğ´ 2: ĞŸĞ¾Ğ¸ÑĞº Ğ² PlayerGui
        local playerGui = LocalPlayer:FindFirstChild("PlayerGui")
        if playerGui then
            for _, gui in pairs(playerGui:GetDescendants()) do
                if gui:IsA("TextBox") then
                    local guiName = gui.Name:lower()
                    if guiName:find("code") or guiName:find("input") then
                        gui.Text = code
                        
                        local parent = gui.Parent
                        if parent then
                            for _, btn in pairs(parent:GetDescendants()) do
                                if btn:IsA("TextButton") then
                                    local btnName = btn.Name:lower()
                                    if btnName:find("redeem") or btnName:find("submit") then
                                        if getconnections then
                                            for _, connection in pairs(getconnections(btn.MouseButton1Click)) do
                                                connection:Fire()
                                            end
                                        end
                                        success = true
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
        
        if success then
            table.insert(RedeemedCodes, code)
            UpdateStatus("âœ… Redeemed: " .. code, Color3.fromRGB(100, 255, 100))
            UpdateCodesStats()
        else
            table.insert(RedeemedCodes, code)
            UpdateCodesStats()
        end
    end)
    
    return success
end

-- Auto Redeem Codes Ñ„ÑƒĞ½ĞºÑ†Ğ¸Ñ
spawn(function()
    wait(3)
    
    while wait(2) do
        if not ScriptEnabled then break end
        if Settings.AutoRedeemCodes then
            local allRedeemed = true
            
            for _, code in ipairs(ActiveCodes) do
                if not ScriptEnabled or not Settings.AutoRedeemCodes then break end
                
                if not table.find(RedeemedCodes, code) then
                    allRedeemed = false
                    UpdateStatus("ğŸ Redeeming: " .. code, Color3.fromRGB(255, 200, 100))
                    
                    local success = RedeemCode(code)
                    if success then
                        game.StarterGui:SetCore("SendNotification", {
                            Title = "âœ… Code Redeemed!";
                            Text = code;
                            Duration = 2;
                        })
                    end
                    
                    wait(3)
                end
            end
            
            if allRedeemed and #RedeemedCodes > 0 then
                UpdateStatus("ğŸ‰ All " .. #RedeemedCodes .. " codes redeemed!", Color3.fromRGB(100, 255, 100))
                Settings.AutoRedeemCodes = false
                AutoRedeemButton.BackgroundColor3 = Color3.fromRGB(255, 75, 75)
                AutoRedeemButton.Text = "ğŸ Auto Redeem Codes: OFF"
                
                game.StarterGui:SetCore("SendNotification", {
                    Title = "ğŸ‰ Complete!";
                    Text = "All codes redeemed successfully!";
                    Duration = 5;
                })
            end
            
            UpdateCodesStats()
        end
    end
end)

-- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ Auto Redeem
AutoRedeemButton.MouseButton1Click:Connect(function()
    if not ScriptEnabled then return end
    
    Settings.AutoRedeemCodes = not Settings.AutoRedeemCodes
    
    if Settings.AutoRedeemCodes then
        AutoRedeemButton.BackgroundColor3 = Color3.fromRGB(75, 255, 75)
        AutoRedeemButton.Text = "ğŸ Auto Redeem Codes: ON"
        
        TweenService:Create(AutoRedeemButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(75, 255, 75)}):Play()
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "ğŸ Auto Redeem Active";
            Text = "Redeeming " .. #ActiveCodes .. " codes...";
            Duration = 3;
        })
    else
        AutoRedeemButton.BackgroundColor3 = Color3.fromRGB(255, 75, 75)
        AutoRedeemButton.Text = "ğŸ Auto Redeem Codes: OFF"
        
        TweenService:Create(AutoRedeemButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 75, 75)}):Play()
    end
end)

-- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ
CloseButton.MouseButton1Click:Connect(function()
    TweenService:Create(MainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 0, 0, 0)}):Play()
    TweenService:Create(MainFrame, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
    
    wait(0.3)
    StopAllScripts()
end)

