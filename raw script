    AccessTitle.Name = "AccessTitle"
    AccessTitle.Parent = AccessFrame
    AccessTitle.BackgroundTransparency = 1
    AccessTitle.Position = UDim2.new(0, 0, 0, 20)
    AccessTitle.Size = UDim2.new(1, 0, 0, 50)
    AccessTitle.Font = Enum.Font.GothamBold
    AccessTitle.Text = "ğŸ”’ Access Required"
    AccessTitle.TextColor3 = Color3.fromRGB(255, 100, 100)
    AccessTitle.TextSize = 28
    AccessTitle.ZIndex = 3
    
    -- ĞĞ¿Ğ¸ÑĞ°Ğ½Ğ¸Ğµ
    AccessDesc.Name = "AccessDesc"
    AccessDesc.Parent = AccessFrame
    AccessDesc.BackgroundTransparency = 1
    AccessDesc.Position = UDim2.new(0, 30, 0, 90)
    AccessDesc.Size = UDim2.new(1, -60, 0, 120)
    AccessDesc.Font = Enum.Font.Gotham
    AccessDesc.Text = "To use this script, please:\n\n1ï¸âƒ£ Click 'Open Profile' button\n2ï¸âƒ£ Add creator to Friends or Follow â­\n3ï¸âƒ£ Return and click 'Verify Access'\n\nâš ï¸ Script will check if you followed!\nThank you for your support! ğŸ’–"
    AccessDesc.TextColor3 = Color3.fromRGB(200, 200, 200)
    AccessDesc.TextSize = 15
    AccessDesc.TextWrapped = true
    AccessDesc.TextYAlignment = Enum.TextYAlignment.Top
    AccessDesc.ZIndex = 3
    
    -- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ¾Ñ‚ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ğ¿Ñ€Ğ¾Ñ„Ğ¸Ğ»Ñ
    OpenProfileButton.Name = "OpenProfileButton"
    OpenProfileButton.Parent = AccessFrame
    OpenProfileButton.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
    OpenProfileButton.BorderSizePixel = 0
    OpenProfileButton.Position = UDim2.new(0.1, 0, 0, 230)
    OpenProfileButton.Size = UDim2.new(0.8, 0, 0, 50)
    OpenProfileButton.Font = Enum.Font.GothamBold
    OpenProfileButton.Text = "ğŸŒ Open Creator Profile"
    OpenProfileButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    OpenProfileButton.TextSize = 18
    OpenProfileButton.ZIndex = 3
    
    local openCorner = Instance.new("UICorner")
    openCorner.CornerRadius = UDim.new(0, 10)
    openCorner.Parent = OpenProfileButton
    
    -- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸
    VerifyButton.Name = "VerifyButton"
    VerifyButton.Parent = AccessFrame
    VerifyButton.BackgroundColor3 = Color3.fromRGB(75, 255, 75)
    VerifyButton.BorderSizePixel = 0
