-- Waste-Time Auto Redeem Codes with Access Check
-- ĞĞ²Ñ‚Ğ¾Ğ¼Ğ°Ñ‚Ğ¸Ñ‡ĞµÑĞºĞ°Ñ Ğ°ĞºÑ‚Ğ¸Ğ²Ğ°Ñ†Ğ¸Ñ Ğ¿Ñ€Ğ¾Ğ¼Ğ¾ĞºĞ¾Ğ´Ğ¾Ğ² Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¾Ğ¹ Ğ´Ğ¾ÑÑ‚ÑƒĞ¿Ğ°

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local TweenService = game:GetService("TweenService")

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ĞĞĞ¡Ğ¢Ğ ĞĞ™ĞšĞ˜ Ğ¡ĞĞ—Ğ”ĞĞ¢Ğ•Ğ›Ğ¯
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local CREATOR_USER_ID = 5682494405 -- User ID ÑĞ¾Ğ·Ğ´Ğ°Ñ‚ĞµĞ»Ñ ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ°
local CREATOR_PROFILE_URL = "https://www.roblox.com/users/" .. CREATOR_USER_ID .. "/profile"
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- Ğ¤Ğ»Ğ°Ğ³Ğ¸
local ScriptEnabled = true
local AccessGranted = true

-- ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸
local Settings = {
    AutoRedeemCodes = false
}

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸ Ğ¿Ğ¾Ğ´Ğ¿Ğ¸ÑĞºĞ¸/Ğ´Ñ€ÑƒĞ·ĞµĞ¹
local function CheckIfFollowing()
    print("ğŸ” Checking if user is following creator...")
    
    -- ĞœĞµÑ‚Ğ¾Ğ´ 1: ĞŸÑ€Ğ¾Ğ²ĞµÑ€ĞºĞ° Ñ‡ĞµÑ€ĞµĞ· HttpGet (ĞµÑĞ»Ğ¸ Ğ´Ğ¾ÑÑ‚ÑƒĞ¿ĞµĞ½)
    if game.HttpGet then
        local success, result = pcall(function()
            local url = "https://friends.roblox.com/v1/users/" .. LocalPlayer.UserId .. "/friends"
            local response = game:HttpGet(url)
            local HttpService = game:GetService("HttpService")
            local data = HttpService:JSONDecode(response)
            
            if data and data.data then
                for _, friend in pairs(data.data) do
                    if friend.id == CREATOR_USER_ID then
                        print("âœ… User is friends with creator!")
                        return true
                    end
                end
            end
            
            return true
        end)
        
        if success and result then
            return true
        end
        
        -- ĞŸÑ€Ğ¾Ğ²ĞµÑ€ĞºĞ° Ğ¿Ğ¾Ğ´Ğ¿Ğ¸ÑÑ‡Ğ¸ĞºĞ¾Ğ²
        local success2, result2 = pcall(function()
            local url = "https://friends.roblox.com/v1/users/" .. CREATOR_USER_ID .. "/followers"
            local response = game:HttpGet(url)
            local HttpService = game:GetService("HttpService")
            local data = HttpService:JSONDecode(response)
            
            if data and data.data then
                for _, follower in pairs(data.data) do
                    if follower.id == LocalPlayer.UserId then
                        print("âœ… User is following creator!")
                        return true
                    end
                end
            end
            
            return false
        end)
        
        if success2 and result2 then
            return true
        end
    end
    
    -- ĞœĞµÑ‚Ğ¾Ğ´ 2: ĞŸÑ€Ğ¾Ğ²ĞµÑ€ĞºĞ° Ñ‡ĞµÑ€ĞµĞ· LocalPlayer.IsFriendsWith (Ğ²ÑÑ‚Ñ€Ğ¾ĞµĞ½Ğ½Ñ‹Ğ¹ Ğ¼ĞµÑ‚Ğ¾Ğ´)
    local success3, result3 = pcall(function()
        return LocalPlayer:IsFriendsWith(CREATOR_USER_ID)
    end)
    
    if success3 and result3 then
        print("âœ… User is friends with creator (IsFriendsWith)!")
        return true
    end
    
    print("âŒ User is not following/friends with creator")
    return false
end

