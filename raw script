    verifyCorner.CornerRadius = UDim.new(0, 10)
    verifyCorner.Parent = VerifyButton
    
    -- ĞšĞ½Ğ¾Ğ¿ĞºĞ° Ğ¿Ñ€Ğ¾Ğ¿ÑƒÑĞºĞ° (Ğ´Ğ»Ñ Ñ‚ĞµÑÑ‚Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ¸Ñ - Ğ¼Ğ¾Ğ¶ĞµÑˆÑŒ ÑƒĞ´Ğ°Ğ»Ğ¸Ñ‚ÑŒ ÑÑ‚Ñƒ ÑĞµĞºÑ†Ğ¸Ñ)
    SkipButton.Name = "SkipButton"
    SkipButton.Parent = AccessFrame
    SkipButton.BackgroundTransparency = 1
    SkipButton.Position = UDim2.new(0, 0, 1, -30)
    SkipButton.Size = UDim2.new(1, 0, 0, 25)
    SkipButton.Font = Enum.Font.Gotham
    SkipButton.Text = "Skip (Testing Only)"
    SkipButton.TextColor3 = Color3.fromRGB(150, 150, 150)
    SkipButton.TextSize = 12
    SkipButton.ZIndex = 3
    
    -- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ Ğ¾Ñ‚ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ğ¿Ñ€Ğ¾Ñ„Ğ¸Ğ»Ñ
    OpenProfileButton.MouseButton1Click:Connect(function()
        TweenService:Create(OpenProfileButton, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(120, 120, 255)}):Play()
        wait(0.1)
        TweenService:Create(OpenProfileButton, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(100, 100, 255)}):Play()
        
        -- ĞšĞ¾Ğ¿Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ ÑÑÑ‹Ğ»ĞºĞ¸ Ğ² Ğ±ÑƒÑ„ĞµÑ€ Ğ¾Ğ±Ğ¼ĞµĞ½Ğ°
        if setclipboard then
            setclipboard(CREATOR_PROFILE_URL)
            OpenProfileButton.Text = "âœ… Link Copied! Open in Browser"
        end
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "ğŸ“‹ Profile Link";
            Text = "Link copied! Paste in browser.\nUser ID: " .. CREATOR_USER_ID;
            Duration = 5;
        })
    end)
    
    -- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸
    VerifyButton.MouseButton1Click:Connect(function()
        VerifyButton.Text = "ğŸ”„ Verifying..."
        VerifyButton.BackgroundColor3 = Color3.fromRGB(255, 200, 100)
        
        wait(1)
        
        -- ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ğ¿Ğ¾Ğ´Ğ¿Ğ¸ÑĞºÑƒ
        local isFollowing = CheckIfFollowing()
        
        if isFollowing then
            -- Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½
            AccessGranted = true
            
            VerifyButton.Text = "âœ… Verified!"
            VerifyButton.BackgroundColor3 = Color3.fromRGB(75, 255, 75)
            
            game.StarterGui:SetCore("SendNotification", {
                Title = "âœ… Access Granted!";
                Text = "Thank you for your support!";
                Duration = 3;
            })
            
            wait(1)
            
            -- ĞĞ½Ğ¸Ğ¼Ğ°Ñ†Ğ¸Ñ Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ
            TweenService:Create(AccessFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 0, 0, 0)}):Play()
            TweenService:Create(Overlay, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
            
            wait(0.3)
            AccessGui:Destroy()
            
            -- Ğ—Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ° Ğ¾ÑĞ½Ğ¾Ğ²Ğ½Ğ¾Ğ³Ğ¾ ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ°
            LoadMainScript()
        else
            -- Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰ĞµĞ½
            VerifyButton.Text = "âŒ Not Following!"
            VerifyButton.BackgroundColor3 = Color3.fromRGB(255, 75, 75)
            
            game.StarterGui:SetCore("SendNotification", {
                Title = "âŒ Access Denied";
                Text = "Please add creator to Favorites first!";
                Duration = 5;
            })
            
            -- Ğ¢Ñ€ÑÑĞºĞ° ĞºĞ½Ğ¾Ğ¿ĞºĞ¸
            for i = 1, 3 do
                TweenService:Create(VerifyButton, TweenInfo.new(0.1), {Position = UDim2.new(0.1, -10, 0, 295)}):Play()
                wait(0.1)
                TweenService:Create(VerifyButton, TweenInfo.new(0.1), {Position = UDim2.new(0.1, 10, 0, 295)}):Play()
                wait(0.1)
            end
            TweenService:Create(VerifyButton, TweenInfo.new(0.1), {Position = UDim2.new(0.1, 0, 0, 295)}):Play()
            
            wait(1)
            VerifyButton.Text = "âœ… Verify Access"
            VerifyButton.BackgroundColor3 = Color3.fromRGB(75, 255, 75)
        end
    end)
    
    -- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ Ğ¿Ñ€Ğ¾Ğ¿ÑƒÑĞºĞ° (ÑƒĞ´Ğ°Ğ»Ğ¸ ÑÑ‚Ñƒ ÑĞµĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ¿Ñ€Ğ¾Ğ´Ğ°ĞºÑˆĞµĞ½Ğ°)
    SkipButton.MouseButton1Click:Connect(function()
        AccessGranted = true
        AccessGui:Destroy()
        LoadMainScript()
    end)
    
    -- ĞĞ½Ğ¸Ğ¼Ğ°Ñ†Ğ¸Ñ Ğ¿Ğ¾ÑĞ²Ğ»ĞµĞ½Ğ¸Ñ
    AccessFrame.Size = UDim2.new(0, 0, 0, 0)
    Overlay.BackgroundTransparency = 1
    TweenService:Create(AccessFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Size = UDim2.new(0, 500, 0, 400)}):Play()
    TweenService:Create(Overlay, TweenInfo.new(0.5), {BackgroundTransparency = 0.3}):Play()
end

